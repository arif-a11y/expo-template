{
  "extends": "expo/tsconfig.base.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@features/*": [
        "./features/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@providers/*": [
        "./providers/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@lib/*": [
        "./lib/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "@config/*": [
        "./config/*"
      ],
      "@assets/*": [
        "./assets/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}